{
  "name": "tpg-dashboard",
  "version": "3.0.0",
  "description": "Enhanced TPG Dashboard with advanced integrations and real-time features",
  "scripts": {
    "dev": "webpack serve --mode development",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "lint": "eslint assets/src/js/**/*.js",
    "lint:fix": "eslint assets/src/js/**/*.js --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write assets/src/js/**/*.js",
    "analyze": "webpack-bundle-analyzer dist/main.js",
    "serve": "concurrently \"npm run watch\" \"npm run dev:server\"",
    "dev:server": "php -S localhost:8000",
    "clean": "rm -rf wp-content/themes/Happy\\ Place\\ Theme/assets/dist/*"
  },
  "dependencies": {
    "chart.js": "^4.4.0",
    "date-fns": "^3.6.0",
    "fabric": "^6.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.0",
    "prettier": "^3.2.5",
    "sass": "^1.89.2",
    "sass-loader": "^14.2.1",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "keywords": [
    "wordpress",
    "dashboard",
    "real-estate",
    "airtable",
    "mls",
    "integrations"
  ],
  "author": "TPG Development Team",
  "license": "GPL-2.0",
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setup.js"],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/assets/src/js/$1",
      "^@dashboard/(.*)$": "<rootDir>/assets/src/js/dashboard/$1",
      "^@components/(.*)$": "<rootDir>/assets/src/js/components/$1",
      "^@integrations/(.*)$": "<rootDir>/assets/src/js/integrations/$1",
      "^@utils/(.*)$": "<rootDir>/assets/src/js/utils/$1"
    }
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
