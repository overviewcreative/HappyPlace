{
    "key": "group_community_core",
    "title": "Core Community Information",
    "fields": [
        {
            "key": "field_year_established",
            "label": "Year Established",
            "name": "year_established",
            "type": "number",
            "instructions": "Year the community was established or built",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            },
            "min": 1800,
            "max": 2030,
            "step": 1
        },
        {
            "key": "field_total_homes",
            "label": "Total Number of Homes",
            "name": "total_homes",
            "type": "number",
            "instructions": "Approximate total number of homes in the community",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            },
            "min": 1,
            "max": 10000,
            "step": 1
        },
        {
            "key": "field_homes_available",
            "label": "Homes Currently Available",
            "name": "homes_available",
            "type": "number",
            "instructions": "Number of homes currently for sale (auto-calculated from listings)",
            "required": 0,
            "readonly": 1,
            "wrapper": {
                "width": "50",
                "class": "hph-readonly-field",
                "id": ""
            }
        },
        {
            "key": "field_financial_info_tab",
            "label": "💰 Financial Information",
            "name": "",
            "type": "tab",
            "placement": "top",
            "endpoint": 0
        },
        {
            "key": "field_hoa_fees",
            "label": "HOA Fees",
            "name": "hoa_fees",
            "type": "text",
            "instructions": "Monthly HOA fees range or amount",
            "required": 0,
            "wrapper": {
                "width": "33",
                "class": "",
                "id": ""
            },
            "placeholder": "e.g., $150/month, $100-$200/month"
        },
        {
            "key": "field_hoa_includes",
            "label": "HOA Includes",
            "name": "hoa_includes",
            "type": "checkbox",
            "instructions": "What HOA fees cover",
            "required": 0,
            "wrapper": {
                "width": "33",
                "class": "",
                "id": ""
            },
            "choices": {
                "landscaping": "Landscaping",
                "lawn_care": "Lawn Care",
                "snow_removal": "Snow Removal",
                "trash_pickup": "Trash Pickup",
                "recycling": "Recycling",
                "security": "Security Services",
                "gate_maintenance": "Gate Maintenance",
                "common_area_maintenance": "Common Area Maintenance",
                "pool_maintenance": "Pool Maintenance",
                "clubhouse_maintenance": "Clubhouse Maintenance",
                "exterior_maintenance": "Exterior Maintenance",
                "insurance": "Master Insurance Policy",
                "utilities": "Some Utilities",
                "cable_internet": "Cable/Internet"
            },
            "layout": "vertical"
        },
        {
            "key": "field_pricing_details",
            "label": "Pricing Information",
            "name": "pricing_details",
            "type": "group",
            "instructions": "Current market pricing in the community",
            "required": 0,
            "wrapper": {
                "width": "34",
                "class": "",
                "id": ""
            },
            "sub_fields": [
                {
                    "key": "field_median_home_price",
                    "label": "Median Home Price",
                    "name": "median_home_price",
                    "type": "number",
                    "instructions": "Typical home price in the community",
                    "wrapper": {"width": "100"},
                    "step": 1000,
                    "prepend": "$"
                },
                {
                    "key": "field_price_range",
                    "label": "Price Range",
                    "name": "price_range",
                    "type": "text",
                    "instructions": "Price range of homes",
                    "wrapper": {"width": "100"},
                    "placeholder": "e.g., $250k - $750k"
                }
            ]
        },
        {
            "key": "field_contact_info_tab",
            "label": "📞 Contact Information",
            "name": "",
            "type": "tab",
            "placement": "top",
            "endpoint": 0
        },
        {
            "key": "field_hoa_contact",
            "label": "HOA Contact Email",
            "name": "hoa_contact",
            "type": "email",
            "instructions": "Contact email for HOA inquiries",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            }
        },
        {
            "key": "field_hoa_phone",
            "label": "HOA Phone Number",
            "name": "hoa_phone",
            "type": "text",
            "instructions": "Contact phone for HOA",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            }
        },
        {
            "key": "field_property_mgmt_contact",
            "label": "Property Management Contact",
            "name": "property_mgmt_contact",
            "type": "email",
            "instructions": "Property management company email",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            }
        },
        {
            "key": "field_property_mgmt_phone",
            "label": "Property Management Phone",
            "name": "property_mgmt_phone",
            "type": "text",
            "instructions": "Property management company phone",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            }
        },
        {
            "key": "field_developer_info",
            "label": "Developer Information",
            "name": "developer_info",
            "type": "group",
            "instructions": "Information about the community developer",
            "required": 0,
            "sub_fields": [
                {
                    "key": "field_developer_name",
                    "label": "Developer Name",
                    "name": "developer_name",
                    "type": "text",
                    "wrapper": {"width": "50"}
                },
                {
                    "key": "field_developer_contact",
                    "label": "Developer Contact",
                    "name": "developer_contact",
                    "type": "text",
                    "wrapper": {"width": "50"},
                    "placeholder": "Phone or email"
                },
                {
                    "key": "field_developer_website",
                    "label": "Developer Website",
                    "name": "developer_website",
                    "type": "url",
                    "wrapper": {"width": "100"}
                }
            ]
        },
        {
            "key": "field_media_tab",
            "label": "📸 Media & Maps",
            "name": "",
            "type": "tab",
            "placement": "top",
            "endpoint": 0
        },
        {
            "key": "field_community_gallery",
            "label": "Community Gallery",
            "name": "community_gallery",
            "type": "gallery",
            "instructions": "Photos showcasing the community",
            "required": 0,
            "return_format": "array",
            "preview_size": "medium",
            "min": 0,
            "max": 50
        },
        {
            "key": "field_video_url",
            "label": "Community Video URL",
            "name": "video_url",
            "type": "url",
            "instructions": "YouTube, Vimeo, or other video tour link",
            "required": 0,
            "placeholder": "https://www.youtube.com/watch?v=..."
        },
        {
            "key": "field_map_embed",
            "label": "Map Embed URL",
            "name": "map_embed",
            "type": "url",
            "instructions": "Google Maps embed URL for the community",
            "required": 0,
            "placeholder": "https://www.google.com/maps/embed?..."
        },
        {
            "key": "field_virtual_tour_url",
            "label": "Virtual Community Tour",
            "name": "virtual_tour_url",
            "type": "url",
            "instructions": "Link to virtual tour of community amenities",
            "required": 0
        },
        {
            "key": "field_status_tab",
            "label": "📊 Status & Availability",
            "name": "",
            "type": "tab",
            "placement": "top",
            "endpoint": 0
        },
        {
            "key": "field_community_status",
            "label": "Community Status",
            "name": "community_status",
            "type": "select",
            "instructions": "Current status of the community",
            "required": 0,
            "choices": {
                "established": "Established",
                "active_development": "Active Development", 
                "new_construction": "New Construction",
                "planned": "Planned/Coming Soon",
                "sold_out": "Sold Out",
                "resale_only": "Resale Only"
            },
            "default_value": "established",
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            }
        },
        {
            "key": "field_new_construction_available",
            "label": "New Construction Available",
            "name": "new_construction_available",
            "type": "true_false",
            "instructions": "Are new homes still being built?",
            "required": 0,
            "wrapper": {
                "width": "50",
                "class": "",
                "id": ""
            },
            "message": "Yes, new construction is available"
        },
        {
            "key": "field_builder_info",
            "label": "Builder Information",
            "name": "builder_info",
            "type": "repeater",
            "instructions": "Active builders in the community",
            "required": 0,
            "conditional_logic": [
                [
                    {
                        "field": "field_new_construction_available",
                        "operator": "==",
                        "value": "1"
                    }
                ]
            ],
            "min": 0,
            "max": 10,
            "layout": "table",
            "button_label": "Add Builder",
            "sub_fields": [
                {
                    "key": "field_builder_name",
                    "label": "Builder Name",
                    "name": "builder_name",
                    "type": "text",
                    "wrapper": {"width": "30"}
                },
                {
                    "key": "field_builder_contact",
                    "label": "Contact Info",
                    "name": "builder_contact",
                    "type": "text",
                    "wrapper": {"width": "30"}
                },
                {
                    "key": "field_builder_price_range",
                    "label": "Price Range",
                    "name": "builder_price_range",
                    "type": "text",
                    "wrapper": {"width": "25"},
                    "placeholder": "$300k-$500k"
                },
                {
                    "key": "field_builder_active",
                    "label": "Active",
                    "name": "builder_active",
                    "type": "true_false",
                    "wrapper": {"width": "15"},
                    "message": "Currently building",
                    "default_value": 1
                }
            ]
        }
    ],
    "location": [
        [
            {
                "param": "post_type",
                "operator": "==",
                "value": "community"
            }
        ]
    ],
    "menu_order": 1,
    "position": "normal",
    "style": "default",
    "label_placement": "top",
    "instruction_placement": "label",
    "active": true,
    "description": "Essential community information including description, pricing, contact details, and media.",
    "show_in_rest": true
}
